<%= render 'shared/header' %>


<div class="container">

  <h1>編集</h1>



    <%= form_for @goodnew do |f| %>

    <%= f.label :タイトル %><br>
    <%= f.text_field :title %><br>

    <%= f.label :URL %><br>
    <%= f.url_field :linkurl %><br>

    <%= f.label :エクセプト %><br>
    <%= f.text_area :except ,:size=>"30x10"%><br>

    <%= f.label :リンクイメージ %><br>
    <%= f.file_field :linkimage ,:size=>"130x10"%><br>



    <%= f.submit '送信' %>



    <% end %>

  </div>




<%= render 'shared/footer' %>
