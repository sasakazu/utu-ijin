<%= render 'shared/header' %>


<div class="container">
  <h1>偉人new</h1>

  <%= form_for @ijin do |f| %>

  <%= f.label :名前 %><br>
  <%= f.text_field :name %><br>

  <%= f.label :イメージ %><br>
  <%= f.file_field :ijinimage %><br>

  <%= f.label :コンテンツ %><br>
  <%= f.text_area :contents ,:size=>"130x10"%><br>



  <%= f.submit '送信' %>



  <% end %>














</div>





<%= render 'shared/footer' %>
